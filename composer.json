{
    "name": "ticketpark/barcode-bundle",
    "type": "symfony-bundle",
    "description": "This bundle assists in creating barcodes and qr codes",
    "keywords": ["barcode", "qrcode"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ticketpark contributors and others",
            "homepage": "https://github.com/Ticketpark/BarcodeBundle/contributors"
        }
    ],

    "require": {
        "php": ">=5.3.2",
        "aferrandini/phpqrcode": "1.0.1",
        "ticketpark/file-bundle": "dev-master",
        "zendframework/zend-barcode": "2.2.*@dev",
        "zendframework/zend-servicemanager": "2.3.*@dev",
        "zendframework/zend-validator": "2.2.*@dev"
    },

    "require-dev": {
        "twig/twig": ">=1.0,<2.0-dev"
    },

    "suggest": {
        "twig/twig": ">=1.0,<2.0-dev"
    },

    "autoload": {
        "psr-0": { "Ticketpark\\BarcodeBundle": "" }
    },

    "target-dir": "Ticketpark/BarcodeBundle"
}
